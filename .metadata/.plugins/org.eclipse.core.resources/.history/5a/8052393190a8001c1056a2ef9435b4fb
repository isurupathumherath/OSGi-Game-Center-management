package com.gamecenter.sa.osgi.gamemanageservice;

public interface GameManageService {
	public int addGame(String gameName, String gameProvider, String gameYear, Double gamePrice);
	public int updateGame(String updateGameName, String updateGameProvider, String updateGameYear, Double updateGamePrice);
	public int removeGame(String gameName);
	public int searchGame(String gameName);
	public List<Game> listGame();
}